{
	"name": "df_dim_radiobase",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_tabla_llamadas",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ds_silver_tabla_datos",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_dim_radiobase",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "union1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          radiobase as string,",
				"          celda as string,",
				"          latitud as decimal(10,6),",
				"          longitud as decimal(10,6)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT DISTINCT rbs_utilizada as radiobase, celda, latitud, longitud FROM silver.datos\\n',",
				"     format: 'query') ~> source1",
				"source(output(",
				"          radiobase as string,",
				"          celda as string,",
				"          latitud as decimal(10,6),",
				"          longitud as decimal(10,6)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT DISTINCT rbs_utilizada_a as radiobase, celda_a as celda, latitud_a as latitud, longitud_a as longitud FROM silver.llamadas\\nUNION\\nSELECT DISTINCT rbs_utilizada_b, celda_b, latitud_b, longitud_b FROM silver.llamadas',",
				"     format: 'query') ~> source2",
				"source1, source2 union(byName: true)~> union1",
				"union1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_radiobase as integer,",
				"          radiobase as string,",
				"          celda as string,",
				"          latitud as decimal(10,6),",
				"          longitud as decimal(10,6)",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}