{
	"name": "df_gold_hechos_datos",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_tabla_datos",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_numero",
						"type": "DatasetReference"
					},
					"name": "dimNumero"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_radiobase",
						"type": "DatasetReference"
					},
					"name": "dimRadioBase"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_fecha",
						"type": "DatasetReference"
					},
					"name": "dimFecha"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_gold_hechos_datos",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "lookup1"
				},
				{
					"name": "lookup2"
				},
				{
					"name": "lookup3"
				},
				{
					"name": "select1"
				},
				{
					"name": "lookup4"
				}
			],
			"scriptLines": [
				"source(output(",
				"          fecha_inicio as timestamp,",
				"          fecha_fin as timestamp,",
				"          numero as string,",
				"          imei as string,",
				"          rbs_utilizada as string,",
				"          celda as string,",
				"          latitud as decimal(18,0),",
				"          longitud as decimal(18,0),",
				"          subidos as integer,",
				"          descargados as integer,",
				"          total as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source(output(",
				"          id_numero as integer,",
				"          numero as string,",
				"          imei as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimNumero",
				"source(output(",
				"          id_radiobase as integer,",
				"          radiobase as string,",
				"          celda as string,",
				"          latitud as decimal(10,6),",
				"          longitud as decimal(10,6)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimRadioBase",
				"source(output(",
				"          id_fecha as integer,",
				"          fecha as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> dimFecha",
				"source1, dimNumero lookup(source1@numero == dimNumero@numero",
				"     && source1@imei == dimNumero@imei,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1, dimRadioBase lookup(rbs_utilizada == radiobase",
				"     && source1@celda == dimRadioBase@celda,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup2",
				"lookup2, dimFecha lookup(fecha_inicio == fecha,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup3",
				"lookup3 select(mapColumn(",
				"          fecha_inicio,",
				"          fecha_fin,",
				"          numero = source1@numero,",
				"          imei = source1@imei,",
				"          rbs_utilizada,",
				"          celda = source1@celda,",
				"          latitud = source1@latitud,",
				"          longitud = source1@longitud,",
				"          subidos,",
				"          descargados,",
				"          total,",
				"          id_numero,",
				"          numero = dimNumero@numero,",
				"          imei = dimNumero@imei,",
				"          id_radiobase,",
				"          radiobase,",
				"          celda = dimRadioBase@celda,",
				"          latitud = dimRadioBase@latitud,",
				"          longitud = dimRadioBase@longitud,",
				"          id_fecha_inicio = id_fecha,",
				"          fecha_i = fecha",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1, dimFecha lookup(fecha_fin == fecha,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup4",
				"lookup4 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_datos as integer,",
				"          subidos as integer,",
				"          descargados as integer,",
				"          total as integer,",
				"          id_fecha_inicio as integer,",
				"          id_fecha_fin as integer,",
				"          id_numero as integer,",
				"          id_radiobase as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          subidos,",
				"          descargados,",
				"          total,",
				"          id_fecha_inicio,",
				"          id_fecha_fin = id_fecha,",
				"          id_numero,",
				"          id_radiobase",
				"     )) ~> sink1"
			]
		}
	}
}